// @flow
import {Component} from 'react'

import * as Constants from '../../../constants/chat'
import type {MessageID} from '../../../constants/chat'

export type PopupEnabledMessage = Constants.TextMessage | Constants.AttachmentMessage

export type Props = {
  message: PopupEnabledMessage,
  onDeleteMessage: (message: PopupEnabledMessage) => void,
  onEditMessage: (message: PopupEnabledMessage) => void,
  onLoadAttachment: (messageID: MessageID, filename: string) => void,
  onOpenInFileUI: (path: string) => void,
  onHidden: () => void,
  style?: Object,
}

export default class Popup extends Component<void, Props, void> { }
