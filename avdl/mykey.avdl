
@namespace("keybase.1")
protocol mykey {

	import idl "common.avdl";

        record PgpCreateUids {
		boolean useDefault;
		array<PgpIdentity> ids;
        }

	void keyGen(
		int primaryBits,
		int subkeyBits,
		PgpCreateUids createUids,
		boolean noPassphrase,
		boolean kbPassphrase,
		boolean noNaclEddsa,
		boolean noNaclDh,
		string pregen);

	void keyGenDefault(
		PgpCreateUids createUids,
		boolean pushPublic,
		boolean pushSecret,
		string passphrase);

	void deletePrimary();
}
