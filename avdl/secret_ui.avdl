
@namespace("keybase.1")
protocol secretUi {
	import idl "common.avdl";

	record SecretEntryArg {
		string desc;
		string prompt;
		string err;
		string cancel;
		string ok;
	}

	record SecretEntryRes {
		string text;
		boolean canceled;
	}

	SecretEntryRes getSecret(SecretEntryArg pinentry, union { null, SecretEntryArg } terminal);
}
