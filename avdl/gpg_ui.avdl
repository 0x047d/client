@namespace("keybase.1")

protocol gpgUi {
	import idl "common.avdl";

	record GPGKey {
		string algorithm;
		string keyID;
		string expiration;
		array<string> identities;	
	}

	record GPGKeySet {
		array<GPGKey> keys;
	}

	record SelectKeyRes {
		string keyID;
	}

	SelectKeyRes SelectKey(int sessionId, GPGKeySet keyset);
}
