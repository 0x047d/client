@namespace("keybase.1")

protocol gpgUi {
	import idl "common.avdl";

	record GPGKey {
		string algorithm;
		string keyID;
		string creation;
		string expiration;
		array<string> identities;
	}

	record SelectKeyRes {
		string keyID;
		boolean doSecretPush;
	}

	boolean wantToAddGPGKey();
	SelectKeyRes selectKeyAndPushOption(int sessionId, array<GPGKey> keys);
	string selectKey(int sessionId, array<GPGKey> keys);
}
