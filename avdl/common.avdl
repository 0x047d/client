
@namespace("keybase.1")
protocol Common {

	record Status {
		int code;
		string name;
		string desc;
		array<string> fields;
	}

	fixed UID(16);
	
	record LoadUserArg {
		union { null, UID } uid;	
		union { null, string} username;
		boolean self;
	}

	record FOKID {
		union { null, bytes } pgpFingerprint;
		union { null, bytes } kid;
	}
}	
