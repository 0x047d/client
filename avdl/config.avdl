
@namespace("keybase.1")
protocol config {

	import idl "common.avdl";

	record GetCurrentStatusArg {
	}

	record GetCurrentStatusResBody {
		boolean configured;
		boolean registered;
		boolean loggedIn;
		boolean publicKeySelected;
		boolean hasPrivateKey;
	}

	record GetCurrentStatusRes {
		union { null, GetCurrentStatusResBody } body;
		Status status;
	}

	GetCurrentStatusRes GetCurrentStatus(GetCurrentStatusArg arg);
}
