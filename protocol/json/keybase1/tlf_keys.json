{
  "protocol": "tlfKeys",
  "imports": [
    {
      "path": "common.avdl",
      "type": "idl"
    }
  ],
  "types": [
    {
      "type": "record",
      "name": "CanonicalTlfName",
      "fields": [],
      "typedef": "string"
    },
    {
      "type": "record",
      "name": "CryptKey",
      "fields": [
        {
          "type": "int",
          "name": "KeyGeneration"
        },
        {
          "type": "Bytes32",
          "name": "Key"
        }
      ]
    },
    {
      "type": "record",
      "name": "TLFCryptKeys",
      "fields": [
        {
          "type": "TLFID",
          "name": "tlfID"
        },
        {
          "type": "CanonicalTlfName",
          "name": "CanonicalName"
        },
        {
          "type": {
            "type": "array",
            "items": "CryptKey"
          },
          "name": "CryptKeys"
        }
      ]
    },
    {
      "type": "record",
      "name": "TLFCanonicalID",
      "fields": [
        {
          "type": "TLFID",
          "name": "tlfID"
        },
        {
          "type": "CanonicalTlfName",
          "name": "CanonicalName"
        }
      ]
    }
  ],
  "messages": {
    "getTLFCryptKeys": {
      "request": [
        {
          "name": "tlfName",
          "type": "string"
        }
      ],
      "response": "TLFCryptKeys",
      "doc": "getTLFCryptKeys returns TLF crypt keys from all generations."
    },
    "getTLFCanonicalID": {
      "request": [
        {
          "name": "tlfName",
          "type": "string"
        },
        {
          "name": "public",
          "type": "boolean"
        }
      ],
      "response": "TLFCanonicalID",
      "doc": "getTLFCanonicalID return the canonical name and TLFID for tlfName."
    }
  },
  "namespace": "keybase.1"
}